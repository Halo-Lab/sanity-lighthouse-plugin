    "react": "^18",
    "sanity": "^3",
    "styled-components": ">= 3"
  },
  "engines": {
    "node": ">=14"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "tsc": false,
      "packageName": false
    }
  }
